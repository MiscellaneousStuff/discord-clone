{
  "name": "discord-clone",
  "version": "1.0.0",
  "description": "A discord clone written in Node.js",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiscellaneousStuff/discord-clone.git"
  },
  "keywords": [
    "discord",
    "clone",
    "node",
    "js",
    "node-js"
  ],
  "author": "MiscellaneousStuff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MiscellaneousStuff/discord-clone/issues"
  },
  "homepage": "https://github.com/MiscellaneousStuff/discord-clone#readme",
  "dependencies": {
    "concurrently": "^7.1.0",
    "express": "^4.18.1",
    "moment": "^2.29.3",
    "socket.io": "^4.5.0",
    "uuid": "^8.3.2"
  }
}
